generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id                        String   @id
  created_at                DateTime @default(now())
  accumulated_rings_points  BigInt      @default(0)
  unclaimed_rings_points    BigInt      @default(0)
  given_blast_poins         BigInt      @default(0)
  pending_blast_points      BigInt      @default(0)
  given_blast_gold_points   BigInt      @default(0)
  pending_blast_gold_points BigInt      @default(0)
  updated_at                DateTime @default(now()) @updatedAt
}
